function [theta, Jh] = GradienteDescenso(X,Y,theta,alpha,iteraciones)
    m = length(Y);
    Jh = zeros(iteraciones,1);
    
    for i = 1: iteraciones
        h = X*theta;
        t1 = theta(1) - alpha*(1/m)*(h-Y)*ones(numel((h-Y)),1);
        t2 = theta(2) - alpha*(1/m)*((h-Y).*X(:,2))*ones(numel((h-Y).*X(:,2)),1);
        theta(1) = t1;
        theta(2) = t2;
        Jh(i) = CostoComputacional(X,Y,theta);
    end
end
